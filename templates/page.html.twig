{% if cme_header %}
	<div id="cme-header-wrapper">
		<div class="container">
			<div id="cme-header" class="col-sm-12">
				<a href="/">
					<img src="https://s3.amazonaws.com/naccme/naccme/hmp_cme_only%402x.png" />
				</a>
			</div>
		</div>
	</div>
{% endif %}
<div id="header-wrapper" class="pColor">
	<section id="header" class="container {% if cme_header %}with-header{%endif%}">
		<div id="header-left" class="col-sm-8">
			    <div id="logo"><a href="{{homepath}}">
			    	<img src="{{brand_logo}}"/>
			    </a></div>
			</div>
			<div id="header-right" class="col-sm-4">
				{{ page.user_menu }}
				<!--<a class="fColor" href="/user/login?destination=" id="link-login">Login</a>
				<a class="fColor" href="/user/register?destination=" id="link-register">Register</a>		  	
				-->
			</div>
		</div>
	</section>
</div>
<div class="sColor" id="header-border"></div>


<section id="main" class="container">
	<section id="course-nav-mobile">{{ page.course_nav_mobile }}</section>
	<div id="page-content" class="{% if page.aside %}col-sm-8{% else %}col-sm-12{% endif %}">
		{{ page.content }}
	</div>
	{% if page.aside %}
	<div id="sidebar" class="col-sm-4">
		{{ page.aside }}
	</div>
	{% endif %}
</section>

